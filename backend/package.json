{
  "name": "api-gestao-crosby",
  "version": "2.0.0",
  "description": "API refatorada para sistema de gestão Crosby",
  "main": "index.refatorado.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node index.refatorado.js",
    "dev": "NODE_ENV=development nodemon index.refatorado.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "security:audit": "npm audit --audit-level=moderate",
    "db:check": "node -e \"import('./config/database.js').then(db => db.default.query('SELECT 1').then(() => console.log('✅ Database connected')).catch(e => console.error('❌ Database error:', e)))\""
  },
  "dependencies": {
    "axios": "^1.6.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "api",
    "express",
    "gestao",
    "crosby",
    "postgresql",
    "jwt"
  ],
  "author": "Sistema Crosby",
  "license": "Private",
  "repository": {
    "type": "git",
    "url": "."
  }
}
